entity Tranfers{
   tranferDate Instant
   comment String
   isApproved Boolean
   isRecieved Boolean
   status Status
   //userId
   // sourceWarehouseid Long
    //destWarehouseid Long
    freeField1 String
    freeField2 String
    lastModified Instant
    lastModifiedBy String
  }
 
  entity TranferDetails{
   approvalDate Instant
   //transferId
   //raw materialId
   //userId requester
   //Source warehouseid
   //destination warehouseid
    qty Double
    comment String
    freeField1 String
    freeField2 String
    lastModified Instant
    lastModifiedBy String
    isDeleted Boolean
    isActive Boolean
  }
 
  entity TranferDetailsApprovals{
    approvalDate Instant
   //raw materialId
   //TransferiD
   //userId approver's
    qtyRequested Double
    qtyApproved Double
    comment String
    freeField1 String
    freeField2 String
    lastModified Instant
    lastModifiedBy String
    isDeleted Boolean
    isActive Boolean
  }
 
   entity TranferRecieved{
    transferDate Instant
    //raw materialId
    //TransferiD
    //userId receiver's
    qtyTransfered Double
    qtyReceived Double
    comment String
    freeField1 String
    freeField2 String
    lastModified Instant
    lastModifiedBy String
    isDeleted Boolean
    isActive Boolean
  }

enum Status {
    REQUESTED(Requested),
    APPROVED(Approved),
    CANCELED(Canceled),
    RECEIVED(Received),
    COMPLETED(Completed)
}

